@model template_partialclass.Models.Product
<div class="col-12 col-md-4 col-lg-3 mb-5 mb-md-0">
	<a class="product-item" href="cart.html">
		<img src="@Model.Image" class="img-fluid product-thumbnail">
		<h3 class="product-title">@Model.productName</h3>
		<strong class="product-price">@Model.Price</strong>

		
	</a>
</div>